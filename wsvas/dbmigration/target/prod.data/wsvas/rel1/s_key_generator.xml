<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <preConditions>
        <dbms type="mysql" />
    </preConditions>
    <changeSet id="create.table" author="lx">
        <createTable tableName="s_key_generator">
            <column autoIncrement="true" name="id" type="int" valueNumeric="1">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="prefix" type="varchar(100)" >
            </column>
            <column name="num" type="INTEGER">
            </column>
            <column name="suffix" type="varchar(100)" >
            </column>
            <column name="length" type="INTEGER">
            </column>
            <column name="key_date" type="TIMESTAMP">
            </column>
            <column name="type" type="varchar(100)">
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>