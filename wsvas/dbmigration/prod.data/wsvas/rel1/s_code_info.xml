<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
    <preConditions>
        <dbms type="mysql" />
    </preConditions>
    <changeSet id="create.table" author="lx">
        <createTable tableName="s_code_info">
            <column autoIncrement="true" name="id" type="int" valueNumeric="1">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="code_type_id" type="varchar(100)" >
            </column>
            <column name="code_value" type="varchar(100)">
            </column>
            <column name="code_name" type="varchar(100)">
            </column>
            <column name="sort_no" type="int(11)" >
            </column>
            <column name="remark" type="varchar(100)">
            </column>

        </createTable>
    </changeSet>
</databaseChangeLog>